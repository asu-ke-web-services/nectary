{
    "name": "gios-asu/nectary",
    "description": "A simple PHP framework that is not tied to a web interface",
    "license": "MIT",
    "version": "0.2.2",
    "authors": [
        {
            "name": "The Julie Ann Wrigley Global Institute of Sustainability at Arizona State University",
            "email": "giosweb@asu.edu"
        }
    ],
    "autoload": {
        "files": [
            "src/utilities/array-utilities.php",
            "src/utilities/command-utilities.php",
            "src/utilities/configuration-utilities.php",
            "src/utilities/dao-utilities.php",
            "src/utilities/date-utilities.php",
            "src/utilities/ensure-default.php",
            "src/utilities/string-utilities.php",
            "src/utilities/view-utilities.php"
        ],
        "classmap" : [ "src/" ]
    },
    "require": {
        "php": ">=7.0",
        "xamin/handlebars.php": "0.10.3",
        "coduo/php-humanizer": ">=2.0.0-beta",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
    	"squizlabs/php_codesniffer": "^2.0",
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": ">=0.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.3",
        "theseer/phpdox": "^0.8",
        "wp-coding-standards/wpcs": "~0.14.0",
        "wimg/php-compatibility": "*"
    }
}
